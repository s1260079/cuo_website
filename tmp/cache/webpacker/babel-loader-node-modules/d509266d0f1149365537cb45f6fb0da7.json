\n    </table>\n    <!-- 詳細 -->\n    <div class=\"row\" v-show=\"userInfoBool\">\n      <div>\n        <div>\n          <div>\n            <p> </p>\n            <span class=\"card-title\">\n              【{{ userInfo.username }}】\n            </span>\n            <div class=\"detail\">\n              ・パート：{{ userInfo.part }}\n            </div>\n            <div class=\"detail\">\n              ・好きなバンド、ジャンルなど：{{ userInfo.likeBand }}\n            </div>\n            <div class=\"detail\">\n              ・一言：{{ userInfo.message }}\n            </div>\n             <v-btn v-if='hantei' class=\"btn #e53935 red darken-1\" v-on:click=\"deleteUser(user.id)\">削除</v-btn>\n          </div>\n        </div>\n      </div>\n    </div>\n    </div>\n  </div>\n</template>\n\n<script>\n  import Vue from 'vue'\n  import axios from 'axios'\n  import firebase from 'firebase'\n  import Set from '../components/set.vue';\n\n  export default {\n    components: {\n      Set,\n   },\n    name: 'Home',\n    data: function() {\n      return {\n        count: 0,\n        counterS: 0,\n        counterM: 0,\n        counterA: 0,\n        counterB: 0,\n        counterK: 0,\n        t: '',\n        a:'',\n        idS: [],\n        idM: [],\n        idA: [],\n        idB: [],\n        idK: [],\n        idZ: [],\n        hantei: '',\n        use:'',\n        email:'',\n        userInfo: {},\n        userInfoBool: false,\n        users: [\n          // username= '',\n          // password= '',\n          // email='',\n          // part= '',\n          // likeBand= '',\n          // message= '',\n          // studentId= '',\n        ],\n        authenticatedUser: false,\n      }\n    },\n    mounted: function() {\n      this.firebase();\n      this.fetchUsers();\n      this.Hanbetsu();\n    },\n    methods: {\n      firebase: function(){//ログインしたユーザーのメールアドレスをもらう\n        this.use = firebase.auth().currentUser;\n        if (this.use != null) {\n            this.email = this.use.email;\n            if(this.email == 's1260079@u-aizu.ac.jp'){\n              this.hantei= true;\n            }\n        }\n      },\n      fetchUsers() {//データベースから登録ユーザーの情報をひっぱる\n        axios.get('/api/users').then((res) => {\n          for(var i = 0; i < res.data.users.length; i++) {\n            this.users.push(res.data.users[i]);\n            this.count += 1;\n          }\n        }, (error) => {\n          console.log(error);\n        });\n      },\n      setUserInfo(id){//登録ユーザーの詳細を表示\n        this.authenticatedUser = '';\n        axios.get(`api/users/${id}.json`).then(res => {\n          this.userInfo = res.data;\n          this.userInfoBool = true;\n          if (this.userInfo.email == this.email) {\n             this.authenticatedUser = true;\n          }else {\n             this.authenticatedUser = false;\n      }\n        });\n      },\n      deleteUser(id) {//ユーザーの消去\n      axios.delete(`/api/users/${id}`).then(res => {\n        this.users = [];\n        this.userInfo = '';\n        this.userInfoBool = false;\n        this.fetchUsers();\n        })\n      },\n      setMemo: function (mem) {\n      this.memos = mem\n      }\n    },\n  }\n</script>\n\n<style scoped>\n.c{\n  width: 800px;\n  margin: auto;\n}\nh1 {\n    color: black;\n    /* background-color: #F8F8FF; */\n    font-weight: 900;\n    margin-top: 40px;\n    margin-bottom: 40px;\n    font-size: 30px;\n  }\ntable {\n  background-color: #F8F8FF;\n}\n.row {\n  background-color: white;\n}\n</style>\n"],"sourceRoot":"app/javascript/views"},"metadata":{},"sourceType":"module"}